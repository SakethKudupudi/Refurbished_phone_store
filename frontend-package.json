{
  "name": "mobile-parts-frontend",
  "version": "1.0.0",
  "description": "Angular frontend for Mobile Parts E-Commerce Platform",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
